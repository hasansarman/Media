{
  "name": "idavoll/media-module",
  "type": "asgard-module",
  "description": "Media module for AsgardCMS. Handles the media library.",
  "keywords": [
    "asgardcms",
    "media",
    "files",
    "library",
    "thumbnails",
    "filters"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Platform/issues",
    "source": "https://github.com/AsgardCms/Media"
  },
  "require": {
    "php": ">=5.5",
    "composer/installers": "*",
    "intervention/image": "*",
    "idavoll/tag-module": "*",
    "guzzlehttp/psr7": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "orchestra/testbench": "*",
    "league/flysystem-aws-s3-v3": "*",
    "mockery/mockery": "*",
    "phpro/grumphp": "*",
    "friendsofphp/php-cs-fixer": "*",
    "doctrine/dbal": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Media\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
